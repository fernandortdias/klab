worldview im;

@test(
	name = "States USA",
	description  = "Spatial data generation from vanilla WCS",
	observations = ("policy:State earth:Region"),
	resources = ("local:ferdinando.villa:test.network:states")
)
// TODO add observe states.shp#alabama as earth:Region;
observe earth:Region named test
   over space(shape = "EPSG:4326 POLYGON ((-124.73142200000001 24.955967, -66.969849 24.955967, -66.969849 49.371735, -124.73142200000001 49.371735, -124.73142200000001 24.955967))");

// this for making the objects available to search   
@index(name=[id], description=description) 
model each local:ferdinando.villa:test.network:states as policy:State earth:Region;