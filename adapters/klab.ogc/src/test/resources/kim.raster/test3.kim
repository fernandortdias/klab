worldview im;

@test(
	name = "tests.core.wcs.0",
	description  = "Spatial data generation from vanilla WCS",
	observations = ("geography:Elevation"),
	resources = ("local:anonymous:test:dem90m"),
	assertions = (
		assert.nodata.percentage(state="geography:Elevation", max=0)
	)
)
observe earth:Region named test
   over space(shape = "EPSG:4326 POLYGON ((-124.73142200000001 24.955967, -66.969849 24.955967, -66.969849 49.371735, -124.73142200000001 49.371735, -124.73142200000001 24.955967))");
     
// within the test project, just say "model dem90m as geography:Elevation in m"
model each local:anonymous:test:states as policy:State;
