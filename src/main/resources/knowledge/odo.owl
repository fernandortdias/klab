    <!-- http://integratedmodelling.org/odo#baseDeclaration -->

    <owl:AnnotationProperty rdf:about="http://integratedmodelling.org/odo#baseDeclaration"/>
    


    <!-- http://integratedmodelling.org/odo#isAbstract -->

    <owl:AnnotationProperty rdf:about="http://integratedmodelling.org/odo#isAbstract"/>
    


    <!-- http://integratedmodelling.org/odo#isDeniable -->

    <owl:AnnotationProperty rdf:about="http://integratedmodelling.org/odo#isDeniable">
        <rdfs:comment>Used to indicate a trait that can be denied.</rdfs:comment>
    </owl:AnnotationProperty>
    


    <!-- http://integratedmodelling.org/odo#isSubjective -->

    <owl:AnnotationProperty rdf:about="http://integratedmodelling.org/odo#isSubjective"/>
    


    <!-- http://integratedmodelling.org/odo#isTypeDelegate -->

    <owl:AnnotationProperty rdf:about="http://integratedmodelling.org/odo#isTypeDelegate">
        <rdfs:comment>Added to &quot;type&quot; delegates that contextualize traits and were not directly declared by users, so that they can be recognized and translated in output and visualization.</rdfs:comment>
    </owl:AnnotationProperty>
    


    <!-- http://integratedmodelling.org/odo#orderingRank -->

    <owl:AnnotationProperty rdf:about="http://integratedmodelling.org/odo#orderingRank"/>
    


    <!-- http://integratedmodelling.org/odo#unit -->

    <owl:AnnotationProperty rdf:about="http://integratedmodelling.org/odo#unit">
        <odo:unit>kg,N</odo:unit>
        <rdfs:comment>Note that kg is allowed as the SI unit, although the actual SI unit for weight (a force) is the Newton (N). It would be quite confusing for now to mandate N for a majority of users.</rdfs:comment>
    </owl:AnnotationProperty>
    


