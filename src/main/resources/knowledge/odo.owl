        <rdfs:domain rdf:resource="http://integratedmodelling.org/odo#Type"/>
        <rdfs:range rdf:resource="http://integratedmodelling.org/odo#Trait"/>
    </owl:ObjectProperty>
    


    <!-- http://integratedmodelling.org/odo#hasAttribute -->

