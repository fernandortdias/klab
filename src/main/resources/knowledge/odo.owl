    <owl:ObjectProperty rdf:about="http://integratedmodelling.org/odo#describesQuality"/>
    


    <!-- http://integratedmodelling.org/odo#discretizesQuality -->

    <owl:ObjectProperty rdf:about="http://integratedmodelling.org/odo#discretizesQuality"/>
    


    <!-- http://integratedmodelling.org/odo#exposesTrait -->

